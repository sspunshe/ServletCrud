<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegisterOrganization</servlet-name>
  	<display-name>RegisterOrganization</display-name>
  	<description></description>
  	<servlet-class>com.controller.RegisterOrganization</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginOrganization</servlet-name>
  	<display-name>LoginOrganization</display-name>
  	<description></description>
  	<servlet-class>com.controller.LoginOrganization</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutOrganization</servlet-name>
  	<display-name>LogoutOrganization</display-name>
  	<description></description>
  	<servlet-class>com.controller.LogoutOrganization</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddUser</servlet-name>
  	<display-name>AddUser</display-name>
  	<description></description>
  	<servlet-class>com.controller.AddUser</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterOrganization</servlet-name>
  	<url-pattern>/RegisterOrg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginOrganization</servlet-name>
  	<url-pattern>/LoginOrg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutOrganization</servlet-name>
  	<url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddUser</servlet-name>
  	<url-pattern>/AddUser</url-pattern>
  </servlet-mapping>
</web-app>
